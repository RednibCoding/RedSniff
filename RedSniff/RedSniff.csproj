<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <ApplicationIcon>Assets\shark.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="accept.png" />
    <None Remove="heading.png" />
    <None Remove="lineNumber.png" />
    <None Remove="restart.png" />
    <None Remove="start.png" />
    <None Remove="stop.png" />
    <None Remove="text.png" />
    <None Remove="undo.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Assets\shark.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\accept.png" />
    <Resource Include="Assets\heading.png" />
    <Resource Include="Assets\lineNumber.png" />
    <Resource Include="Assets\restart.png" />
    <Resource Include="Assets\start.png" />
    <Resource Include="Assets\stop.png" />
    <Resource Include="Assets\text.png" />
    <Resource Include="Assets\undo.png" />
    <Resource Include="Assets\shark_big.png" />
  </ItemGroup>

</Project>
